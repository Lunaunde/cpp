    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "C:/mingw64/bin/gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/P1012.cpp",
                "-o",
                "${workspaceFolder}/Debug/P1012.exe",
                "-lstdc++"
            ],
            "group": "build",
            "problemMatcher": [
                "$gcc"
            ]
        },
